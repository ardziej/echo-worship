      "parts": [
        [
          {
            "text": "Nasz Ojcze nieskończony"
          },
          {
            "text": "Wszystko stworzyłeś Ty"
          },
          {
            "text": "Wszechmogący"
          }
        ],
        [
          {
            "text": "Świętego Ducha mocą"
          },
          {
            "text": "Poczęty został Syn"
          },
          {
            "text": "Jezus, nasz Zbawca"
          }
        ]
      ]
    },
    {
      "id": "c1",
      "parts": [
        [
          {
            "text": "Tak, ja wierzę w Boga Ojca"
          },
          {
            "text": "W Syna, którym Jezus jest"
          }
        ],
        [
          {
            "text": "Wierzę też w Świętego Ducha"
          },
          {
            "text": "Bóg jest w osobach trzech"
          }
        ],
        [
          {
            "text": "Wierzę także w zmartwychwstanie"
          },
          {
            "text": "Że powstaniemy znów"
          }
        ],
        [
          {
            "text": "Wyznaję to: Wierzę w imię Jezus"
          }
        ]
      ]
    },
    {
      "id": "v2",
      "parts": [
        [
          {
            "text": "Nasz Sędzio, nasz Obrońco"
          },
          {
            "text": "Cierpiałeś za nasz grzech"
          },
          {
            "text": "A nam przebaczono"
          }
        ],
        [
          {
            "text": "Ty zszedłeś do ciemności"
          },
          {
            "text": "I w chwale wzniosłeś się"
          },
          {
            "text": "Zająłeś w niebie tron"
          }
        ]
      ]
    },
    {
      "id": "b",
      "parts": [
        [
          {
            "text": "Wierzę, że Bóg jest"
