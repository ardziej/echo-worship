  "lyrics": [
    {
      "id": "v1",
      "parts": [
        [
          {
            "text": "Wspominam dzień, gdy wziąłeś krzyż"
          },
          {
            "text": "By życie me odkupić z win"
          }
        ],
        [
          {
            "text": "I widzę łzy i rany Twe"
          },
          {
            "text": "Zbawiła mnie Twa święta krew"
          }
        ]
      ]
    },
    {
      "id": "v2",
      "parts": [
        [
          {
            "text": "Gdy ciało Twe opuścił duch"
          },
          {
            "text": "Oddano Ci Józefa grób"
          }
        ],
        [
          {
            "text": "Tam wejścia strzegł ogromny głaz"
          },
          {
            "text": "Mesjaszu nasz, zostałeś sam"
          }
        ]
      ]
    },
    {
      "id": "c",
      "parts": [
        [
          {
            "text": "O, chwalmy Go, to nasz Pan, nasz Bóg"
          },
          {
            "text": "O, chwalmy Go na wieki już"
          }
        ],
        [
          {
            "text": "Bez końca niech wszystko śpiewa Mu"
          },
          {
            "text": "To On, nasz Pan, nasz Bóg"
          }
        ]
      ]
    },
    {
      "id": "post-c",
      "parts": [
        [
          {
            "text": "To On, nasz Pan, nasz Bóg"
          }
        ]
      ]
    },
    {
      "id": "v3",
      "parts": [
        [
          {
            "text": "Trzeciego dnia, gdy nastał świt"
          },
          {
            "text": "Wtedy zmartwychwstał Boży Syn"
          }
        ],
        [
          {
            "text": "Zdeptana śmierć straciła moc"
          },
          {
            "text": "Aniołów głos uwielbia Go"
          }
        ]
      ]
    },
    {
      "id": "v4",
      "parts": [
        [
          {
            "text": "Powróci znów ubrany w biel"
          },
          {
            "text": "I w ciemną noc nastanie dzień"
          }
        ],
        [
          {
            "text": "Wśród świętych też powstanę ja"
          },
          {
            "text": "By patrzeć na Jezusa twarz"
          }
        ]
      ]
    },
    {
      "id": "i",
      "parts": [
        [
          {
            "text": ""
          }
        ]
      ]
    }
