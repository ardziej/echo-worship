  "lyrics": [
    {
      "id": "v1",
      "parts": [
        [
          {
            "text": "Przyjmij serce me"
          },
          {
            "text": "Ręce moje weź"
          }
        ],
        [
          {
            "text": "Chcę w miłości Twej"
          },
          {
            "text": "Znów zanurzyć się"
          }
        ]
      ]
    },
    {
      "id": "pre-c1",
      "parts": [
        [
          {
            "text": "Nawet kiedy nie mam sił"
          },
          {
            "text": "Chcę wielbić"
          }
        ],
        [
          {
            "text": "Nawet kiedy trudno mi"
          },
          {
            "text": "Chcę wielbić"
          }
        ],
        [
          {
            "text": "Nawet kiedy brak mi słów, chcę żyć"
          },
          {
            "text": "Jeszcze głośniej wielbiąc Cię"
          }
        ]
      ]
    },
    {
      "id": "c",
      "parts": [
        [
          {
            "text": "Tylko Ciebie wielbić chcę"
          }
        ],
        [
          {
            "text": "Tylko Ciebie wielbić chcę"
          }
        ],
        [
          {
            "text": "Tylko Ciebie wielbić chcę"
          }
        ]
      ]
    },
    {
      "id": "v2",
      "parts": [
        [
          {
            "text": "Weź to morze łez"
          },
          {
            "text": "Cały ciężar weź"
          }
        ],
        [
          {
            "text": "Mocno trzymaj mnie"
          },
          {
            "text": "Przyjdź znów w łasce swej"
          }
        ]
      ]
    },
    {
      "id": "pre-c2",
      "parts": [
        [
          {
            "text": "Nawet gdy przegrywam bój"
          },
          {
            "text": "Chcę wielbić"
          }
        ],
        [
          {
            "text": "Nawet kiedy czuję ból"
          },
          {
            "text": "Chcę wielbić"
          }
        ],
        [
          {
            "text": "Kiedy nie ma sensu już mój śpiew"
          },
          {
            "text": "Jeszcze głośniej śpiewać chcę"
          }
        ]
      ]
    },
    {
      "id": "b",
      "parts": [
        [
          {
            "text": "Twoim ogniem płonie mój duch"
          },
          {
            "text": "Tylko Ty, tylko Ciebie chcę"
          }
        ],
        [
          {
            "text": "Moja dusza pragnie Twych słów"
          },
          {
            "text": "Śpiewam Ci, aż nadejdzie dzień znów"
          }
        ],
        [
          {
            "text": "Twoim ogniem płonie mój duch"
          },
          {
            "text": "Tylko Ty, tylko Ciebie chcę"
          }
        ],
        [
          {
            "text": "Moja dusza pragnie Twych słów"
          },
          {
            "text": "Śpiewam Ci, aż wydarzy się cud"
          }
        ]
      ]
    },
    {
      "id": "pre-c3",
      "parts": [
        [
          {
            "text": "Nawet gdy nadejdzie dzień"
          },
          {
            "text": "Chcę wielbić"
          }
        ],
        [
          {
            "text": "Kiedy wygram bitwy me"
          },
          {
            "text": "Chcę wielbić"
          }
        ],
        [
          {
            "text": "Gdy mój czas na ziemi skończy się"
          },
          {
            "text": "Będę głośniej wielbić Cię"
          }
        ]
      ]
    },
    {
      "id": "t",
      "parts": [
        [
          {
            "text": "Tylko Ciebie wielbić chcę"
          }
        ]
      ]
    },
