  "lyrics": [
    {
      "id": "v1",
      "parts": [
        [
          {
            "text": "Stwórco istnienia"
          }
        ],
        [
          {
            "text": "Zanim był czas"
          },
          {
            "text": "Zanim rozpoczął się świat"
          }
        ],
        [
          {
            "text": "Gdzie chaos i ciemność"
          },
          {
            "text": "Na dźwięk Twoich słów"
          },
          {
            "text": "Nagle pojawił się blask"
          }
        ]
      ]
    },
    {
      "id": "c1",
      "parts": [
        [
          {
            "text": "I na Twój głos"
          },
          {
            "text": "Galaktyk sto miliardów rodzi się"
          }
        ],
        [
          {
            "text": "A oddech Twój początkiem planet jest"
          },
          {
            "text": "Jeśli gwiazdy Ciebie wielbią, to i ja"
          }
        ],
        [
          {
            "text": "Widzę serce Twoje w każdym z Twoich dzieł"
          },
          {
            "text": "Światło wszystkich gwiazd odbiciem łaski jest"
          }
        ],
        [
          {
            "text": "Jeśli wszechświat Ciebie chwali, to i ja"
          }
        ]
      ]
    },
    {
      "id": "v2",
      "parts": [
        [
          {
            "text": "Boże obietnic"
          }
        ],
        [
          {
            "text": "Twe słowo ma moc"
          },
          {
            "text": "Na wieki nie zmieni się już"
          }
        ],
        [
          {
            "text": "Ty raz przemówiłeś"
          },
          {
            "text": "A nauka i świat"
          },
          {
            "text": "Idą za dźwiękiem Twych słów"
          }
        ]
      ]
    },
    {
      "id": "c2",
      "parts": [
        [
          {
            "text": "I na Twój głos"
          },
          {
            "text": "Ze sto miliardów stworzeń bierze wdech"
          }
        ],
        [
          {
            "text": "By Twój odwieczny plan wypełnił się"
          },
          {
            "text": "Jeśli to objawia Ciebie, to i ja"
          }
        ],
        [
          {
            "text": "Widzę serce Twoje w każdym z Twoich dzieł"
          }
        ],
        [
          {
            "text": "Niebo pełne barw"
          },
          {
            "text": "Obrazem łaski jest"
          }
        ],
        [
          {
            "text": "Jeśli wszystko słucha Ciebie, to i ja"
          }
        ],
        [
          {
            "text": "To i ja"
          },
          {
            "text": "To i ja"
          }
        ]
      ]
    },
    {
      "id": "b",
      "parts": [
        [
          {
            "text": "Jeśli gwiazdy Ciebie wielbią, to i ja"
          },
          {
            "text": "Jeśli góry się kłaniają, to i ja"
          }
        ],
        [
          {
            "text": "Jeśli morza głoszą chwałę, to i ja"
          },
          {
            "text": "Jeśli wszystko Cię wywyższa, Stwórco nasz"
          }
        ],
        [
          {
            "text": "To i ja"
          }
        ],
        [
          {
            "text": "Jeśli wiatr Ci jest posłuszny, to i ja"
          },
          {
            "text": "Jeśli kamień głos podnosi, to i ja"
          }
        ],
        [
          {
            "text": "Wiem, że więcej niż to wszystko jesteś wart"
          },
          {
            "text": "Więc zaśpiewam na Twą chwałę jeszcze raz"
          }
        ]
      ]
    },
    {
      "id": "v3",
      "parts": [
        [
          {
            "text": "Boże zbawienia"
          }
        ],
        [
          {
            "text": "Znalazłeś mnie, gdy"
          },
          {
            "text": "Zgubiła mnie duma i grzech"
          }
        ],
        [
          {
            "text": "Na górze cierpienia"
          },
          {
            "text": "Jedyny Twój Syn"
          },
          {
            "text": "Był za mnie wydany na śmierć"
          }
        ]
      ]
    },
    {
      "id": "c3",
      "parts": [
        [
          {
            "text": "I na Twój głos"
          },
          {
            "text": "Ze sto miliardów błędów znika gdzieś"
          }
        ],
        [
          {
            "text": "Życia zrzekłeś się, by mi darować je"
          },
          {
            "text": "Jeśli Ty powstałeś z martwych, to i ja"
          }
        ],
        [
          {
            "text": "Widzę serce Twoje w każdej z Twoich ran"
          }
        ],
        [
          {
            "text": "Arcydzieło Twe"
          },
          {
            "text": "Na imię Miłość ma"
          }
        ],
        [
          {
            "text": "Jeśli Ty się uniżyłeś, to i ja"
          },
          {
            "text": "Widzę serce Twe w miliardach wszystkich nas"
          }
        ],
        [
          {
            "text": "Każdy cenny jest"
          },
          {
            "text": "Imiona wszystkich znasz"
          }
        ],
        [
          {
            "text": "Jeśli żyłeś, żeby kochać, to i ja"
          }
        ]
      ]
    },
    {
      "id": "tag",
      "parts": [
        [
          {
            "text": "Wiem, że zrobiłbyś to wszystko jeszcze raz"
          },
          {
            "text": "Ukochałeś nas już zanim powstał świat"
          },
          {
            "text": "Ty byś wszystko oddał za jednego z nas"
          }
        ]
      ]
    },
