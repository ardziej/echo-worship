  "lyrics": [
    {
      "id": "v1",
      "parts": [
        [
          {
            "text": "Wieczność we mnie jest"
          },
          {
            "text": "Końcem nie jest śmierć"
          },
          {
            "text": "Wiem, że niebo czeka mnie"
          }
        ],
        [
          {
            "text": "Gdy niełatwo mi"
          },
          {
            "text": "Jesteś przy mnie Ty"
          },
          {
            "text": "Wszystko mam, by wielbić Cię"
          }
        ]
      ]
    },
    {
      "id": "pre-c",
      "parts": [
        [
          {
            "text": "Bo wiem, że znasz mnie"
          },
          {
            "text": "I wiem, że kochasz"
          }
        ],
        [
          {
            "text": "I w swojej łasce"
          },
          {
            "text": "Ocaliłeś mnie na zawsze"
          }
        ],
        [
          {
            "text": "I chociaż czekam"
          },
          {
            "text": "Znam smak nieba"
          }
        ],
        [
          {
            "text": "Bo niebo we mnie jest"
          }
        ]
      ]
    },
    {
      "id": "v2",
      "parts": [
        [
          {
            "text": "Choćby przyszedł ból"
          },
          {
            "text": "Mój dom nie jest tu"
          },
          {
            "text": "Wiem, że niebo czeka mnie"
          }
        ],
        [
          {
            "text": "Choć noc ciemna jest"
          },
          {
            "text": "Ja nie boję się"
          },
          {
            "text": "Wszystko mam, by wielbić Cię"
          }
        ]
      ]
    },
    {
      "id": "c",
      "parts": [
        [
          {
            "text": "Będę śpiewać jak wtedy"
          },
          {
            "text": "Kiedy stanę w Twojej chwale"
          }
        ],
        [
          {
            "text": "Tam gdzie ciemność mnie nie znajdzie"
          },
          {
            "text": "Gdzie zobaczę Twoją twarz"
          }
        ],
        [
          {
            "text": "Będę śpiewać jak ten"
          },
          {
            "text": "Kto nie czuje bólu wcale"
          }
        ],
        [
          {
            "text": "Kto już nie zna łez i zranień"
          },
          {
            "text": "Kto prawdziwie wolny jest"
          }
        ]
      ]
    },
    {
      "id": "b",
      "parts": [
        [
          {
            "text": "Śpiewam: Święty, święty"
          },
          {
            "text": "Mój Bóg jest święty"
          }
        ],
        [
          {
            "text": "Tak jak jest tam, w niebie"
          },
          {
            "text": "Niech we mnie jest"
          }
        ]
      ]
    },
    {
      "id": "b2",
      "parts": [
        [
          {
            "text": "Śpiewam: Święty, święty"
          },
          {
            "text": "Mój Bóg jest święty"
          }
        ],
        [
          {
            "text": "Tak jak jest tam, w niebie"
          },
          {
            "text": "Niech we mnie jest"
          }
        ],
        [
          {
            "text": "Ziemia śpiewa: Święty"
          },
          {
            "text": "Nasz Bóg jest święty"
          }
        ],
        [
          {
            "text": "Tak jak jest tam, w niebie"
          },
          {
            "text": "Niech stanie się"
          }
        ]
      ]
