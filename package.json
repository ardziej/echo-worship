{
  "name": "echo-worship",
  "version": "0.2.0",
  "description": "ECHO Worship",
  "homepage": "http://weareecho.pl",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon app.js",
    "ws": "nodemon ws.js",
    "electron": "electron electron.js",
    "start": "concurrently --kill-others \"yarn run dev\" \"yarn run ws\"",
    "app": "concurrently --kill-others \"yarn run dev\" \"yarn run ws\" \"yarn run electron\"",
    "db-pull": "rm -rf ./s3/db && rclone sync -v echo:echo/echo-worship/db ./s3/db --exclude '.gitkeep' --exclude '.DS_Store'",
    "db-push": "rclone sync -v ./s3/db echo:echo/echo-worship/db --exclude '.gitkeep' --exclude '.DS_Store'",
    "s3": "rclone sync -v echo:echo/echo-worship ./s3 --exclude '.gitkeep' --exclude '.DS_Store'",
    "s3-push": "rclone sync -v ./s3 echo:echo/echo-worship --exclude '.gitkeep' --exclude '.DS_Store'",
    "s3-sync": "rclone sync -v echo:echo/worship-media ./public/media --exclude '.gitkeep' --exclude '.DS_Store'",
    "s3-purge": "rm -rf ./public/media/*",
    "s3-resync": "yarn s3-purge && yarn s3-sync",
    "s3-sync-remote": "rclone sync -v ./public/media echo:echo/worship-media --exclude '.gitkeep' --exclude '.DS_Store'",
    "lint": "eslint src -c .eslintrc --ext js,jsx"
  },
  "author": {
    "name": "Michal Ardziejewski",
    "email": "adjweb@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ardziej/echo-worship.git"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mustache": "^2.3.0",
    "pug": "^2.0.3",
    "socket.io": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^2.0.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "grunt-cli": "^1.2.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.14.2"
  }
}
